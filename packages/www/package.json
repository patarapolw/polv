{
  "name": "@polv/www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'wait-on tcp:$npm_package_config_serverPort && yarn dev:www' 'yarn dev:server' 'yarn dev:lambda'",
    "build": "yarn build:lambda && concurrently 'wait-on tcp:$npm_package_config_serverPort && yarn build:www' 'yarn dev:server'",
    "dev:www": "nuxt",
    "build:www": "nuxt generate",
    "lint:www": "eslint --ext '.ts,.js,.vue' --ignore-path .gitignore .",
    "openapi": "wget http://127.0.0.1:$npm_package_config_serverPort/api/doc/json -O ./assets/openapi.json && typegen ./assets/openapi.json > ./types/openapi.d.ts",
    "dev:server": "NODE_ENV=development ts-node-dev -P server/tsconfig.json -r tsconfig-paths/register server/index.ts",
    "ts": "ts-node -P server/tsconfig.json -r tsconfig-paths/register",
    "prebuild": "yarn ts scripts/upload.ts",
    "dev:lambda": "netlify-lambda serve lambda",
    "build:lambda": "netlify-lambda build lambda",
    "postinstall": "netlify-lambda install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@types/aws-lambda": "^8.10.83",
    "axios": "^0.22.0",
    "concurrently": "^6.2.2",
    "core-js": "^3.18.1",
    "dayjs": "^1.10.7",
    "fastify": "^3.22.0",
    "fastify-swagger": "^4.12.3",
    "jsonschema-definer": "^1.3.2",
    "lunr": "^2.3.9",
    "netlify-lambda": "^2.0.14",
    "nuxt": "^2.15.8",
    "nuxt-buefy": "^0.4.10",
    "nuxt-mq": "^2.0.2",
    "nuxt-property-decorator": "^2.9.1",
    "openapi-client-axios": "^4.1.0",
    "tsconfig-paths": "^3.11.0",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-object-assign": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/proxy": "^2.1.0",
    "@patarapolw/scope-css": "^1.3.0",
    "@types/cheerio": "^0.22.30",
    "@types/ejs": "^3.1.0",
    "@types/js-yaml": "^4.0.3",
    "@types/lunr": "^2.3.4",
    "@types/pino": "^6.3.11",
    "@types/showdown": "^1.9.4",
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.5",
    "ejs": "^3.1.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "fast-glob": "^3.2.7",
    "fastify-cors": "^6.0.2",
    "highlight.js": "^11.2.0",
    "highlightjs-vue": "highlightjs/highlightjs-vue",
    "html-encoder-decoder": "^1.3.9",
    "import-sort-parser-typescript": "^6.0.0",
    "js-yaml": "^4.1.0",
    "nuxt-typed-vuex": "^0.2.0",
    "openapi-client-axios-typegen": "^3.14.1",
    "pino-pretty": "^7.0.1",
    "sass": "^1.42.1",
    "sass-loader": "~10.1.1",
    "showdown": "^1.9.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "config": {
    "serverPort": "17338"
  }
}
