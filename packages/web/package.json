{
  "name": "@patarapolw/polv",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "generate-db": "ts-node -O '{\"module\":\"commonjs\",\"noImplicitAny\":false}' scripts/build.ts",
    "dev": "run-p 'dev:**'",
    "dev:nuxt": "nuxt-ts",
    "dev:lambda": "netlify-lambda serve lambda",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "clean": "run-p 'clean:**'",
    "clean:content": "rimraf content",
    "clean:db": "ts-node -O '{\"module\":\"commonjs\",\"noImplicitAny\":false}' scripts/clean.ts",
    "build": "run-p 'build:**'",
    "build:nuxt": "nuxt-ts build && nuxt-ts export",
    "build:lambda": "netlify-lambda build lambda",
    "postinstall": "netlify-lambda install",
    "deploy": "netlify deploy --dir dist --functions lambda-dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/bulma": "^1.3.0",
    "@types/aws-lambda": "^8.10.64",
    "@types/cheerio": "^0.22.22",
    "@types/ejs": "^3.0.4",
    "@types/fs-extra": "^9.0.2",
    "@types/he": "^1.1.1",
    "@types/js-yaml": "^3.12.5",
    "@types/lunr": "^2.3.3",
    "@types/node-fetch": "^2.5.7",
    "@types/rimraf": "^3.0.0",
    "@types/sharp": "^0.26.0",
    "@types/showdown": "^1.9.3",
    "cheerio": "^1.0.0-rc.3",
    "core-js": "^3.6.5",
    "dayjs": "^1.9.3",
    "dot-prop-immutable": "^2.1.0",
    "ejs": "^3.1.5",
    "fast-glob": "^3.2.4",
    "fs-extra": "^9.0.1",
    "highlight.js": "^10.2.1",
    "js-yaml": "^3.14.0",
    "lunr": "^2.3.9",
    "netlify-lambda": "^2.0.1",
    "node-fetch": "^2.6.1",
    "nuxt": "^2.14.6",
    "nuxt-mq": "^2.0.1",
    "nuxt-property-decorator": "^2.8.8",
    "rimraf": "^3.0.2",
    "scope-css": "github:patarapolw/scope-css",
    "sharp": "^0.26.2",
    "showdown": "^1.9.1",
    "vue-tweet-embed": "^2.4.0",
    "zod": "^1.11.9"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "sass": "^1.27.0",
    "ts-node": "^9.0.0"
  },
  "engines": {
    "node": "12.x",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "5.x"
  },
  "engineStrict": true
