{
  "name": "@polv/buefy-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p 'dev:*'",
    "dev:server": "NODE_ENV=development ts-node-dev -P server/tsconfig.json -r tsconfig-paths/register server/index.ts",
    "dev:www": "nuxt",
    "build": "run-s 'build:*'",
    "build:server": "tsc -P server/tsconfig.json",
    "build:www": "nuxt build",
    "start": "run-p 'start:*'",
    "start:server": "node -r tsconfig-paths/register server-dist/index.js",
    "start:www": "nuxt start",
    "lint": "eslint --ext '.ts,.js,.vue' --ignore-path .gitignore .",
    "ts": "ts-node -P server/tsconfig.json -r tsconfig-paths/register"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@typegoose/typegoose": "^7.6.0",
    "core-js": "^3.8.3",
    "fastify": "^3.14.0",
    "highlight.js": "^10.6.0",
    "jsonschema-definer": "^1.3.2",
    "mongoose": "5.10.18",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.14.12",
    "nuxt-buefy": "^0.4.4",
    "tsconfig-paths": "^3.9.0",
    "wait-on": "^5.2.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/mongoose": "^5.10.3",
    "@types/pino": "^6.3.6",
    "@types/wait-on": "^5.2.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "fastify-cors": "^5.2.0",
    "import-sort-parser-typescript": "^6.0.0",
    "nuxt-mq": "^2.0.2",
    "nuxt-property-decorator": "^2.9.1",
    "nuxt-typed-vuex": "^0.1.22",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "10",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  }
}