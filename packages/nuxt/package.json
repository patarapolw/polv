{
  "name": "polv",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "ts": "ts-node -O '{\"module\":\"commonjs\"}'",
    "db:make": "yarn ts scripts/make.ts",
    "dev": "run-p 'dev:**'",
    "dev:nuxt": "nuxt-ts",
    "dev:lambda": "netlify-lambda serve lambda",
    "prebuild": "yarn db:make",
    "build": "run-p 'build:**'",
    "build:nuxt": "nuxt-ts generate",
    "build:lambda": "netlify-lambda build lambda",
    "postinstall": "netlify-lambda install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.4",
    "@patarapolw/highlightjs-vue": "^1.0.0-dev4",
    "@patarapolw/scope-css": "^1.3.0",
    "@types/aws-lambda": "^8.10.66",
    "@types/cheerio": "^0.22.23",
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.5",
    "@types/js-yaml": "^3.12.5",
    "@types/lunr": "^2.3.3",
    "@types/rimraf": "^3.0.0",
    "@types/showdown": "^1.9.3",
    "cheerio": "^1.0.0-rc.3",
    "core-js": "^3.8.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "fast-glob": "^3.2.4",
    "fs-extra": "^9.0.1",
    "highlight.js": "^10.4.1",
    "js-yaml": "^3.14.1",
    "lunr": "^2.3.9",
    "netlify-lambda": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.14.12",
    "nuxt-buefy": "^0.4.4",
    "nuxt-property-decorator": "^2.8.8",
    "rimraf": "^3.0.2",
    "showdown": "^1.9.1",
    "ts-node": "^9.1.1",
    "zod": "^1.11.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "import-sort-parser-typescript": "^6.0.0",
    "prettier": "^2.2.1"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": "1",
    "pnpm": "please-use-yarn"
  },
  "importSort": {
    ".ts, .tsx, .config.ts": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
